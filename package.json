{
  "name": "immersion-presentation",
  "version": "1.1.5",
  "description": "A presentation framework for making mathematical presentations",
  "author": "gillescastel",
  "license": "MIT",
  "repository": "gillescastel/immersion",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --jsxFragment React.Fragment --no-compress --css-modules false --format modern,cjs && cp ./src/*.macro.js ./dist/",
    "start": "microbundle-crl watch --jsxFragment React.Fragment --no-compress --css-modules false --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-p test:lint test:unit test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "@types/jest": "^26.0.13",
    "@types/lodash": "^4.14.159",
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^4.0.0-alpha.14",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "files": [
    "dist",
    "make-pdf/index.js"
  ],
  "dependencies": {
    "@citation-js/plugin-bibtex": "^0.5.0-alpha.6",
    "@types/gsap": "^1.20.2",
    "citation-js": "^0.5.0-alpha.6",
    "gsap": "^3.5.0",
    "lodash.flattendeep": "^4.4.0",
    "md5": "^2.3.0",
    "memoizee": "^0.4.14",
    "node-fetch": "^2.6.0",
    "react-router-dom": "^5.2.0",
    "svg-path-parse": "^1.1.3",
    "use-resize-observer": "^6.1.0"
  }
}
